NODE total_calories
SQL >

    SELECT
        arrayReduce('sum', arrayFilter((x) -> x != 0, arrayJoin(arraySplit((x) -> x = 0, groupArray(if(isnull (calories), 0, calories)))))) as total
    FROM day_1
    ORDER BY total DESC
    LIMIT 3



NODE part_1
SQL >

    SELECT * FROM total_calories LIMIT 1



NODE part_2
SQL >

    SELECT sum(total) as total FROM total_calories


